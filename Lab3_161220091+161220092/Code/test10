int PerfectNumber()
{
	int p[30];
	int i, num, count, s;
	int MaxNum=500;
	
	num=2;
	while(num < MaxNum) {
		count=0;
		s=num;
		i=1;
		while(i<(num/2+1)){
			if((num - (num / i) * i) == 0) {
				p[count] = i;
				count = count + 1;
				s = s - i;
			}
			i = i + 1;
		}
		if( 0 == s)
		{
			write(num);
			write(p[0]);
			i=1;
			while(i<count) {
				write(p[i]);
				i = i+1;
			}
		}
		num = num + 1;
	}
	return 0;
}
int main() {
	int a = PerfectNumber();
	return 0;
}
