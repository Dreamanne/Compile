struct test {
	int op1;
	int op2;
};
struct temp {
	int a;
	int b;
	int c[10];
	int d;
	struct test test1;
};
int add(struct test t1, struct test t2) {
	int result;
	result = t1.op1 + t2.op1;
	return result;
}
int main() {
	struct temp m;
	struct temp n;
	m.test1.op1 = read();
	n.test1.op1 = read();
	write(add(m.test1, n.test1));
	return 0;
}
